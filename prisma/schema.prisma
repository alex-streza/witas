generator client {
  provider        = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

enum StickerType {
  DALL_E_2
  MIDJOURNEY
}

model stickers {
  id        Int      @id @default(autoincrement())
  url    String
  type    StickerType
  prompt    String
  createdAt DateTime @default(now())
}
