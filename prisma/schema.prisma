generator client {
  provider        = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

enum StickerType {
  DALL_E_2
  MIDJOURNEY
}

model Sticker {
  id        Int      @id @default(autoincrement())
  url    String
  type    String
  prompt    String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
